{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "htmljinja2",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "doctype"
        },
        {
          "type": "SYMBOL",
          "name": "script_element"
        },
        {
          "type": "SYMBOL",
          "name": "style_element"
        },
        {
          "type": "SYMBOL",
          "name": "erroneous_end_tag"
        },
        {
          "type": "SYMBOL",
          "name": "element"
        },
        {
          "type": "SYMBOL",
          "name": "entity"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "_jinja"
        }
      ]
    },
    "doctype": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<!"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_doctype"
          },
          "named": false,
          "value": "doctype"
        },
        {
          "type": "PATTERN",
          "value": "[^>]+"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "_doctype": {
      "type": "PATTERN",
      "value": "DOCTYPE",
      "flags": "i"
    },
    "element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "start_tag"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "end_tag"
                },
                {
                  "type": "SYMBOL",
                  "name": "_implicit_end_tag"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "self_closing_tag"
        }
      ]
    },
    "start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_start_tag_name"
          },
          "named": true,
          "value": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "self_closing_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_start_tag_name"
          },
          "named": true,
          "value": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": "/>"
        }
      ]
    },
    "script_element": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "script_start_tag"
          },
          "named": true,
          "value": "start_tag"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_raw_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "_jinja"
                }
              ]
            }
          },
          "named": true,
          "value": "raw_text"
        },
        {
          "type": "SYMBOL",
          "name": "end_tag"
        }
      ]
    },
    "script_start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_script_start_tag_name"
          },
          "named": true,
          "value": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "style_element": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "style_start_tag"
          },
          "named": true,
          "value": "start_tag"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_raw_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "_jinja"
                }
              ]
            }
          },
          "named": false,
          "value": "raw_text"
        },
        {
          "type": "SYMBOL",
          "name": "end_tag"
        }
      ]
    },
    "style_start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_style_start_tag_name"
          },
          "named": true,
          "value": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "attribute_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "attribute_value"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "quoted_attribute_value"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "attribute_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": -1,
          "content": {
            "type": "PATTERN",
            "value": "[^<>\"'/=\\s\\{]+"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "_jinja"
          }
        }
      ]
    },
    "attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^<>\"'=\\s\\{]+"
        },
        {
          "type": "SYMBOL",
          "name": "_jinja"
        }
      ]
    },
    "quoted_attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "REPEAT1",
                    "content": {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "PATTERN",
                          "value": "[^'\\{]+"
                        },
                        {
                          "type": "PATTERN",
                          "value": "\\{"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "_jinja"
                        }
                      ]
                    }
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              "named": false,
              "value": "attribute_value"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "REPEAT1",
                    "content": {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "PATTERN",
                          "value": "[^\"\\{]+"
                        },
                        {
                          "type": "PATTERN",
                          "value": "\\{"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "_jinja"
                        }
                      ]
                    }
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              "named": false,
              "value": "attribute_value"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
    },
    "end_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_end_tag_name"
          },
          "named": true,
          "value": "tag_name"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "erroneous_end_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "SYMBOL",
          "name": "erroneous_end_tag_name"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "entity": {
      "type": "PATTERN",
      "value": "&(#([xX][0-9a-fA-F]{1,6}|[0-9]{1,5})|[A-Za-z]{1,30});?"
    },
    "_jinja": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_jinja_statement"
        },
        {
          "type": "SYMBOL",
          "name": "jinja_output"
        },
        {
          "type": "SYMBOL",
          "name": "jinja_comment"
        }
      ]
    },
    "_jinja_expression_in_statement": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s\\%\\-\\+]+|[\\%\\-\\+]"
      }
    },
    "jinja_output": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_jinja_output_code"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          "named": true,
          "value": "jinja_expression"
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "_jinja_output_code": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "[^\\s\\}\\-\\+]+|[\\}\\-\\+]"
        }
      }
    },
    "_jinja_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "jinja_for_statement"
        }
      ]
    },
    "jinja_for_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "\\{\\%[\\+\\-]?"
              },
              "named": false,
              "value": "jinja_statement_start"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "STRING",
                    "value": "for"
                  }
                }
              },
              "named": false,
              "value": "keyword"
            },
            {
              "type": "FIELD",
              "name": "iteration",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "_jinja_expression_in_statement"
                },
                "named": true,
                "value": "jinja_expression"
              }
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[\\+\\-]?\\%\\}"
              },
              "named": false,
              "value": "jinja_statement_end"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_node"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "jinja_for_else_statement"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "\\{\\%[\\+\\-]?"
                  },
                  "named": false,
                  "value": "jinja_statement_start"
                },
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "TOKEN",
                    "content": {
                      "type": "PREC",
                      "value": 1,
                      "content": {
                        "type": "STRING",
                        "value": "endfor"
                      }
                    }
                  },
                  "named": false,
                  "value": "keyword"
                },
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[\\+\\-]?\\%\\}"
                  },
                  "named": false,
                  "value": "jinja_statement_end"
                }
              ]
            }
          ]
        }
      ]
    },
    "jinja_for_else_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "\\{\\%[\\+\\-]?"
              },
              "named": false,
              "value": "jinja_statement_start"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "STRING",
                    "value": "else"
                  }
                }
              },
              "named": false,
              "value": "keyword"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[\\+\\-]?\\%\\}"
              },
              "named": false,
              "value": "jinja_statement_end"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_node"
            }
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "\\{\\%[\\+\\-]?"
              },
              "named": false,
              "value": "jinja_statement_start"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "STRING",
                    "value": "endfor"
                  }
                }
              },
              "named": false,
              "value": "keyword"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[\\+\\-]?\\%\\}"
              },
              "named": false,
              "value": "jinja_statement_end"
            }
          ]
        }
      ]
    },
    "jinja_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{#"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^\\#]+|[\\#]"
          }
        },
        {
          "type": "STRING",
          "value": "#}"
        }
      ]
    },
    "text": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": -1,
                "content": {
                  "type": "PATTERN",
                  "value": "\\{"
                }
              }
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": 1,
                "content": {
                  "type": "PATTERN",
                  "value": "[^<>&\\s\\{][^<>&\\{]*[^<>&\\s\\{]?"
                }
              }
            }
          ]
        }
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_start_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "_script_start_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "_style_start_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "_end_tag_name"
    },
    {
      "type": "SYMBOL",
      "name": "erroneous_end_tag_name"
    },
    {
      "type": "STRING",
      "value": "/>"
    },
    {
      "type": "SYMBOL",
      "name": "_implicit_end_tag"
    },
    {
      "type": "SYMBOL",
      "name": "_raw_text"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "inline": [],
  "supertypes": []
}
